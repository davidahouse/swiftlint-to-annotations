{
  "name": "swiftlint-to-annotations",
  "main": "bin/swiftlint-to-annotations.js",
  "version": "1.0.1",
  "description": "A command line app for turning swiftlint output to annotation json for GitHub checks",
  "license": "MIT",
  "repository": "davidahouse/swiftlint-to-annotations",
  "preferGlobal": true,
  "bin": {
    "swiftlint-to-annotations": "bin/swiftlint-to-annotations.js"
  },
  "dependencies": {},
  "devDependencies": {
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0"
  },
  "scripts": {
    "lint": "eslint $(find . -name \"*.js\" -not -path \"./node_modules/*\" -not -path \"./public/*\")",
    "lint-fix": "eslint --fix $(find . -name \"*.js\" -not -path \"./node_modules/*\" -not -path \"./public/*\")"
  }
}
